// Linear Search in 2D array
#include <iostream>
using namespace std;

bool linearSearch(int arr[][3], int target, int rows, int col)
{
  for (int i = 0; i < 3; i++)
  {
    for (int j = 0; j < 3; j++)
    {
      if (arr[i][j] == target)
      {
        return 1;
      }
    }
  }
  return 0;
}
int main()
{

  int arr[3][3] = {{1, 2, 3}, {34, 56, 45}, {33, 0, 7}};

  int target;
  cout << "Enter the element you want to find" << endl;
  cin >> target;

  if (linearSearch)
  {
    cout << "element found" << endl;
  }
  else
  {
    cout << "element not found" << endl;
  }

  return 0;
}
